<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="liemei.Model" namespace="liemei.Model" default-lazy="true">

  <class name="UserJurisdiction" table="UserJurisdiction">
    <id name="ID">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="Jurisdiction" column="Jurisdiction" />
    <property name="UserID" column="UserID" />
  </class>
</hibernate-mapping>