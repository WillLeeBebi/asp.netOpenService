<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="liemei.Model" namespace="liemei.Model" default-lazy="true">

  <class name="UserGroup" table="UserGroup">
    <id name="ID">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="GroupName" column="GroupName" />
    <property name="CreateTime" column="CreateTime"/>
    <property name="PGroupID"   column="PGroupID"   />
    <property name="EnterpriseID"      column="EnterpriseID"      />
  </class>
</hibernate-mapping>