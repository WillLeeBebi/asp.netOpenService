<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="liemei.Model" namespace="liemei.Model" default-lazy="true">

  <class name="WeChatLogin" table="WeChatLogin">
    <id name="ID">
      <column name="ID" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="UUID" column="UUID"  not-null="true"/>

    <property name="Openid" column="Openid"/>

    <property name="Nickname"   column="Nickname"   />

    <property name="Sex"      column="Sex"      />

    <property name="State"    column="State"    />
    <property name="Headimgurl"    column="Headimgurl"    />
    <property name="LoginData"    column="LoginData"    />
    <property name="CreateTime"    column="CreateTime"    />
    <property name="LockCode"    column="LockCode"    />
  </class>
</hibernate-mapping>