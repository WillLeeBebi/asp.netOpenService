@using bnuxq.Model
@model IList<PsychtestBuyLog>
@{
    ViewBag.Title = "收入明细";
    List<UserInfo> userList = ViewBag.userList;
    Layout = "~/Views/Shared/_weChartLayoutNew.cshtml";
}
<div class="weui-cells__title">收入明细</div>


<div class="weui-cells">
    @if (Model != null && Model.Count > 0)
    {
        foreach (var buyLog in Model)
        {
            string username = string.Empty;
            if (userList != null && userList.Count > 0)
            {
                var userinfo = userList.FirstOrDefault(x => x.ID == buyLog.UserID);
                if (userinfo != null)
                {
                    username = userinfo.Nickname;
                }
            }
            <div class="weui-cell">
                <div class="weui-cell__bd">
                    <p>@(username)购买了您分享的『@(buyLog.PsychtestName)』&nbsp;&nbsp;@(buyLog.CreateTime.ToString("yyyy-MM-dd"))</p>
                </div>
                <div class="weui-cell__ft">￥@(buyLog.Money)</div>
            </div>
        }
    }
</div>

