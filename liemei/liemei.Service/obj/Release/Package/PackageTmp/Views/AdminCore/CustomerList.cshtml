                    <div class="weui-media-box__bd">
                        <h4 class="weui-media-box__title">@ar.Name</h4>
                        <p class="weui-media-box__desc">总分:@ar.TotalScore</p>
                        <p class="weui-media-box__desc">等级:@ar.TotalLevelName</p>
                        <p class="weui-media-box__desc">@ar.EvaluationTypeStr</p>
                        <p class="weui-media-box__desc">总分:@ar.TotalScore</p>
                        <span class="boxcs1">@username</span>
                        <span class="fontcolor rightcs">@ar.CreateDate</span>
                    </div>
                </a>
            </div>
        }
