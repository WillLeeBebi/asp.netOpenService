            if (user != null)
            {
                username = user.Name;
                userhand = user.Headimgurl;
            }
            <div class="weui-panel__bd">
                <a href="/EvaluationResultView/Index?erid=@ar.ID" class="weui-media-box weui-media-box_appmsg">
                    <div class="weui-media-box__hd">
