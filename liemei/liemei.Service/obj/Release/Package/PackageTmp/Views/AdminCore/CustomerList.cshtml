<div class="weui-panel weui-panel_access">
    <div class="weui-panel__hd"><span class="titltcs">智能设备报告</span></div>
    @if (Model != null && Model.Count > 0)
    {
        foreach (var ar in Model)
        {
            var user = userlist.FirstOrDefault(x => x.ID == ar.UserID);
